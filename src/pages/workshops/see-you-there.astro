---
import Layout from "@layouts/default";

const content = {
	title: "Pre-Workshop | Amy Kapernick",
	description: "A few questions to prepare for your upcoming workshop",
};
---
<style>
	@import '@mixins';

	article {
		@include landing;
	}
</style>

<Layout content={content} header="reduced">
	<article>
		<h1>See you there</h1>

		<p>Thanks for filling out the form, see you at the workshop!</p>

		<p>If you're looking for some time to kill, see how many HTML elements you can name without looking it up - <a href="https://kapers.dev/html-game" target="_blank">kapers.dev/html-game</a></p>
	</article>
</Layout>
